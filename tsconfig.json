{
	"compilerOptions": {
		// "target": "esnext",
		// "module": "commonjs",
		"moduleResolution": "node",
		"target": "es5",
		"module": "esnext",
		// "moduleResolution": "bundler",
		"lib": ["dom", "dom.iterable", "esnext"],
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"allowJs": true,
		"noEmit": true,
		"incremental": true,
		"declaration": true,
		"plugins": [
			{
				"name": "next"
			}
		],
		"paths": {
			"@/*": ["./*"]
		}
	},
	"include": [
		"next-env.d.ts",
		"**/*.ts",
		"**/*.tsx",
		"**/*.png",
		".next/types/**/*.ts",
		"app/lib/placeholder-data.js",
		"scripts/seed.js"
	],
	"exclude": ["node_modules"]
}
